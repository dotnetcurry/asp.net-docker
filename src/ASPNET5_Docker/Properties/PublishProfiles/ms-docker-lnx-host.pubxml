<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>Custom</WebPublishMethod>
    <publishMethod>Docker</publishMethod>
    <profileName>ms-docker-lnx-host</profileName>
    <DockerAppType>Web</DockerAppType>
    <DockerServerUrl>tcp://ms-docker-lnx-host.southeastasia.cloudapp.azure.com:2376</DockerServerUrl>
    <DockerImageName>aspnet5_docker</DockerImageName>
    <DockerPublishHostPort>80</DockerPublishHostPort>
    <DockerPublishContainerPort>80</DockerPublishContainerPort>
    <DockerBuildOnly>False</DockerBuildOnly>
    <DockerRemoveConflictingContainers>True</DockerRemoveConflictingContainers>
    <DockerAuthOptions>--tlsverify --tlscacert="E:\Mahesh_New\Certificates\ca.pem" --tlscert="E:\Mahesh_New\Certificates\cert.pem" --tlskey="E:\Mahesh_New\Certificates\key.pem"</DockerAuthOptions>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <PublishProvider>Docker</PublishProvider>
    <LastUsedBuildConfiguration />
    <LastUsedPlatform />
    <CompileSource>False</CompileSource>
    <UsePowerShell>True</UsePowerShell>
    <WebRoot>wwwroot</WebRoot>
    <WwwRootOut>wwwroot</WwwRootOut>
    <IncludeSymbols>False</IncludeSymbols>
    <Native>False</Native>
    <IgnoreDNXRuntime>True</IgnoreDNXRuntime>
    <DockerfileRelativePath>..\..\Dockerfile</DockerfileRelativePath>
    <_DefaultDNXVersion>dnx-clr-win-x86.1.0.0-rc1-update1</_DefaultDNXVersion>
    <IISCommand>web</IISCommand>
    <PublishPreviewUrl>tcp://ms-docker-lnx-host.southeastasia.cloudapp.azure.com:2376</PublishPreviewUrl>
  </PropertyGroup>
</Project>